'______________________GIROS______________________

Sub giro2m3
  While Sensor.ReadPercent(3) > targeti
    Motor.StartSync("BC",pot, -(pot))
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub giro2m2
  While Sensor.ReadPercent(2) > targetd
    Motor.StartSync("BC",pot, -(pot))
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub giro2mgrad
  Motor.ResetCount("B")
  While math.Abs(Motor.GetCount("B")) < grados
    Motor.StartSync("BC",pot, -(pot))
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub giromder3
