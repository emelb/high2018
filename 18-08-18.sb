      Speaker.Note(100,"C4",100)
      avance = Motor.GetCount("C")
      
      Motor.StartPower("BC",-70)
      Program.Delay(100)
      Motor.Stop("BC","TRUE")
      
      If barco <> color Then
        pos_barco=6
      Else
        If avance <= 526 Then
          pos_barco=2
        EndIf
        If avance > 526 And avance <= 853 Then
          pos_barco=3
        EndIf
        If avance > 853 And avance <= 1196 Then
          pos_barco=4
        EndIf
        If avance > 1196 Then
          pos_barco=5
        EndIf
      EndIf
    EndIf
    
    LCD.Clear()
    LCD.Text(1,30,60,2,pos_barco)
    
    DejarBarcos()
  EndIf
EndSub

