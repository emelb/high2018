'___________________SEGUIDORES_____________
Sub segizqg
  Motor.ResetCount("B")
  While(Motor.GetCount("B")<grados)
    err=Sensor.ReadPercent(3) - targeti
    u=kp*err+(err-errold)*kd
    Motor.StartPower("B",pot-u)
    Motor.StartPower("C",pot+u)
    Program.Delay(1)
    errold=err
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub segderg
  Motor.ResetCount("B")
  While(Motor.GetCount("B")<grados)
    err=targetd - Sensor.ReadPercent(2)
    u=kp*err+(err-errold)*kd
    Motor.StartPower("B",pot-u)
    Motor.StartPower("C",pot+u)
    Program.Delay(1)
    errold=err
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub segder
  While Sensor.ReadPercent(3)>targeti
    err=targetd - Sensor.ReadPercent(2)
    u=kp*err+(err-errold)*kd
    Motor.StartPower("B",pot-u)
    Motor.StartPower("C",pot+u)
    Program.Delay(1)
    errold=err
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub segizq
  While Sensor.ReadPercent(2)>targetd
    err=Sensor.ReadPercent(3) - targeti
    u=kp*err+(err-errold)*kd
    Motor.StartPower("B",pot-u)
    Motor.StartPower("C",pot+u)
    Program.Delay(1)
    errold=err
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub seg2grad
  Motor.ResetCount("B")
  While Motor.GetCount("B")<grados
    err=(Sensor.ReadPercent(2)+20)-Sensor.ReadPercent(3)
    u=kp*err+(err-errold)*kd
    Motor.StartPower("B",pot-u)
    Motor.StartPower("C",pot+u)
    errold=err
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub seg2
  While Sensor.ReadPercent(2)>targeti or Sensor.ReadPercent(3)>targetd
    err=(Sensor.ReadPercent(2)+20)-Sensor.ReadPercent(3)
    u=kp*err+(err-errold)*kd
    Motor.StartPower("B",pot-u)
    Motor.StartPower("C",pot+u)
    errold=err
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub segbarco
  While Sensor.ReadRawValue(4,0) = 0
    err=Sensor.ReadPercent(3) - targeti
    u=kp*err+(err-errold)*kd
    Motor.StartPower("B",pot-u)
    Motor.StartPower("C",pot+u)
    Program.Delay(1)
    errold=err
  EndWhile
EndSub

'/////////////////////////////////////////////////////////////////////////////////////

Sub segvacio
  While Sensor.ReadRawValue(4,0) <> 0
    err=Sensor.ReadPercent(3) - targeti
    u=kp*err+(err-errold)*kd
    Motor.StartPower("B",pot-u)
    Motor.StartPower("C",pot+u)
    Program.Delay(1)
    errold=err
  EndWhile
EndSub

