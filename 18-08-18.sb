      errold=0
      While barco <> color And Motor.GetCount("C") < 1400
        err=Sensor.ReadPercent(3) - targeti
        u=kp*err+(err-errold)*kd
        Motor.StartPower("B",pot-u)
        Motor.StartPower("C",pot+u)
        errold=err
        lecturabarco()
